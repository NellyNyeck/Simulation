{
  "$schema" : "http://json-schema.org/draft-07/schema#",
  "description" : "format of edge in the graph",
  "type" : "object",
  "properties" : {
    "name" : {
      "type" : "string"
    },
    "from" : {
      "type" : "string"
    },
    "to" : {
      "type" : "string"
    },
    "weight" : {
      "type" : "number",
      "default" : 1
    },
    "poi distribution function" : {
      "type" : "object",
      "properties" : {
        "name" : {
          "type" : "string"
        },
        "parameters" : {
          "type" : "array",
          "items" : "parameter"
          },
          "minItems" : 1,
          "maxItems" : 3,
          "uniqueItems" : true
        }
      },
      "required" : [
        "name",
        "parameters"
      ]
    },
    "provider" : {
      "type" : "provider"
    }
  },
  "required" : [
    "name",
    "from",
    "to"
  ]
}