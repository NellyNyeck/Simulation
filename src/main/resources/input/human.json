{
  "$schema" : "http://json-schema.org/draft-07/schema#",
  "description" : "the human structure",
  "type": "object",
  "properties" : {
    "name" : {
      "type" : "string"
    },
    "filename" : {
      "type" : "string"
    },
    "agent-type" : {
      "type" : "string",
      "default" : "human"
    },
    "start" :{
      "type":"object",
      "$ref":"node.json"
    },
    "finish" :{
      "type":"object",
      "$ref":"node.json"
    },
    "middle": {
      "type":"array",
      "items": {
        "type" : "object",
        "$ref" : "node.json"
      }
    }
  },
  "required" : [
    "name",
    "filename",
    "agent-type",
    "start",
    "finish"
  ]
}