{
  "$schema" : "http://json-schema.org/draft-07/schema#",
  "description" : "the structure of a provider in this simulation environment",
  "type" : "object",
  "extends" : {
    "$ref" : "2static.json"
  },
  "properties" : {
    "agent-type" : {
      "type" : "string",
      "default" : "provider"
    },
    "colour" : {
      "type" : "string"
    },
    "depots" : {
      "type" : "array",
      "items" : {
        "type" : "node"
      },
      "minItems" : 1,
      "uniqueItems" : true
    },
    "available-pods" : {
      "type" : "integer"
    },
    "pods" : {
      "type" : "array",
      "items" : {
        "type" : "pod"
      },
      "minItems" : 1,
      "uniqueItems" : true
    },
    "maximum number of customers" : {
      "type" : "integer"
    },
    "maximum outgoing pods/time unit" : {
      "type" : "number"
    },
    "solver" : {
      "type" : "object"
    },
    "poi selection function" : {
      "type" : "function"
    }
  },
"required" :["agent-type", "colour", "depots", "available-pods", "maximum number of customers","maximum outgoing pods/time unit", "poi selection function"]
}