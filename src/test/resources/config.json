{
	"definitions":{
		"gps":{
			"type":"object",
			"proprieties":{
				"degree": {"type":"number"},
				"minute": {"type":"number"},
				"second": {"type":"number"}
			},
			"required":["degree","minute","second"]
		},
		"node":{
			"type":"object",
			"properties": {
				"id": { "type":"string"},
				"coordinates": {"type": "object",
					"properties":{
						"type 1":{"type": "string", "default":"geographic"},
						"type 2":{"type": "string", "default":"artificial"},
						"x": {"type":"number"},
						"y": {"type":"number"},
						"latitude":{"type":"gps"},
						"longitude":{"type":"gps"}
						},
						"required":["type"],
						"dependencies":{
							"type 2":["x","y"],
							"type 1":["latitude","longitude"]
						}
					},
				"entry": {"type":"boolean"},
				"capacity":{ "type":"integer", "multipleOf":1.0}
				},
			"required":["id","x","y"],
			"dependencies": {
				"entry": ["capacity"]
			}
		},
		"poi":{
			"type":"object",
			"properties": {
				"id": { "type":"object",
					"proprieties": {
						"id end 1": {"type": "integer", "multipleOf":1.0 },
						"connector 1":{"type":"string", "default":"-"},
						"position x":{"type":"number"},
						"position y":{"type":"number"},
						"connector 2":{"type":"string", "default":"-"},
						"id end 2": {"type": "integer", "multipleOf":1.0}
					},
					"required":["id end 1","connector 1","position x", "position y", "connector 2", "id end 2"]
				}
			},
			"required":["id"]
		},
		"provider":{
			"type":"object",
			"properties": {
				"name": { "type":"string"},
				"colour":{ "type":"string" },
				"depot":{"type":"node"},
				"available pods":{"type":"integer"},
				"pods":{"type":"array",
						"items":{"type":"pod"},
						"minItems": 1,
						"maxItems":"available pods",
						"uniqueItems": true
						},
				"maximum number of customers":{"type":"integer"},
				"maximum outgoing pods/time unit":{"type":"number"},
				"solver":{"type":"object"},
				"poi selection function":{"type":"distribution"}
				
			},
			"required":["name","colour","depot","available pods","maximum number of customers","maximum outgoing pods/time unit","poi selection function"]
		},
		"edge":{
			"type":"object",
			"properties": {
				"id": {"type":"object",
						"properties":{
							"name":{"type":"string"},
							"lane":{"type":"integer"}
						},
						"required":["name"]
					},
				"from": { "type":"node"},
				"to":{ "type":"node" },
				"weight": {"type":"number", "default":1},
				"poi distribution function": {"type":"object",
												"properties":{
													"name":  {"type":"string"},
													"input data": {"type":"array",
																	"items":{"type":"number"},
																	"minItems": 1,
																	"maxItems": 3
																	}
												},
												"required":["name"]
											},
				"provider": { "type":"provider"}
			},
			"required":["from","to"]
		},
		"strategy":{
			"type":"object",
			"properties": {
				"name": { "type":"string"}
			},
			"required":["name"]
		},
		"pod":{
			"type":"object",
			"properties": {
			"id":{"type":"string"},
			"capacity": { "type":"integer", "default":1},
			"provider":{ "type":"provider" },
			"strategy":{"type":"strategy"}
			},
			"required":["id","capacity","strategy","provider"]
		},
		"environment":{
		"type":"object",
		"properties": {
			"nodes": { "type":"array",
				"items": { "type": "node"},
				"minItems": 2,
				"uniqueItems": true
			},
			"edges": { "type":"array",
				"items": { "type": "edge"},
				"minItems": 1,
				"uniqueItems": true
			}
		},
		"required":["nodes","edges"]
		},
		"objects":{
			"type":"object",
			"properties": {
				"pods": { "type":"array",
					"items": { "type": "pod"},
					"minItems": 1,
					"uniqueItems": true
				},
				"providers": {"type":"array",
					"items":{ "type":"provider" },
					"minItems": 1,
					"uniqueItems": true
				}
			}
		},
		"core":{
			"type":"object",
			"properties": {
				"active poi": { "type":"integer", "default":1},
				"poi selection": {"type":"distribution"},
				"simulations":{"type":"integer", "default":10}
			},
			"required":["active poi","poi selection", "simulations"]
		}
	}
}

